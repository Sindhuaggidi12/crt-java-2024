import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int a=sc.nextInt();
    int b=sc.nextInt();


    int c=a*b;
    if(c>1000)
        {
            System.out.printf("%d",a+b);
    }
    else
    {
       System.out.printf("%d",c);
    }
    }
}
